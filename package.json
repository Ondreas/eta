{
    "name": "eta",
    "version": "0.0.1",
    "description": "Personal project",
    "main": "index.html",
    "scripts": {
        "start": "npm-run-all clean dev:css dev:11ty",
        "clean": "rimraf dist",
        "dev:11ty": "cross-env NODE_ENV=development npx @11ty/eleventy --serve --quiet",
        "dev:css": "cross-env NODE_ENV=development postcss src/assets/css/index.css --o dist/assets/css/style.css --config ./postcss.config.js --watch",
        "twcss": "tailwindcss -i src/assets/css/index.css -o dist/assets/css/style.css --watch --postcss",
        "build": "npm-run-all clean -p build:*",
        "build:css": "cross-env NODE_ENV=production postcss src/assets/css/index.css --o dist/assets/css/style.css --config ./postcss.config.js",
        "build:11ty": "cross-env NODE_ENV=production npx @11ty/eleventy"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ondreas/eta.git"
    },
    "keywords": [
        "tailwind",
        "css",
        "tailwindcss",
        "alpine.js",
        "eleventy",
        "11ty"
    ],
    "author": "Ondreas",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Ondreas/eta/issues"
    },
    "homepage": "https://github.com/Ondreas/eta#readme",
    "devDependencies": {
        "@11ty/eleventy": "^2.0.1",
        "autoprefixer": "^10.4.14",
        "cross-env": "^7.0.3",
        "cssnano": "^6.0.0",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.22",
        "postcss-cli": "^10.1.0",
        "postcss-import": "^15.1.0",
        "rimraf": "^5.0.0",
        "tailwindcss": "^3.3.1",
        "tailwindcss-logical": "^3.0.0"
    },
    "dependencies": {
        "@alpinejs/persist": "^3.12.0",
        "alpinejs": "^3.12.0",
        "cssnano-preset-advanced": "^6.0.0"
    }
}
